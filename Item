public class Item {
    
    private String name;
    private int value;
    private int number;
    
    public Item(name, value, number) {
        this.name = name;
        this.value = value;
        this.number = numer;
    }
    
    public String toStrng() {
        System.out.println(name + "x" + number + " (" + value + "g" + ")"); 
    }
}
